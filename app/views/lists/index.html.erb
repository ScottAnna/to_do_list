<%= render '/layouts/navbar' %>

<h1>All Tasks</h1>



<%= render partial: 'index_list', locals: {list: @list} %>


<h4><%= link_to 'New List', new_list_path %><h4>


<% @lists.each do |list| %>
  <h4><%= list.name %>
  <%= link_to list do %><i class="glyphicon glyphicon-cloud"></i><% end %>
  <%= link_to edit_list_path(list) do %><i class="glyphicon glyphicon-pencil"></i><% end %></h4>
<% end %>
